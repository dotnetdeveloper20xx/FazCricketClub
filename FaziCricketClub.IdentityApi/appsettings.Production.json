{
  "ConnectionStrings": {
    "IdentityConnection": "Server=YOUR_PROD_SERVER;Database=CricketClubIdentityDb;User Id=YOUR_USER;Password=YOUR_PASSWORD;MultipleActiveResultSets=true;Encrypt=True;TrustServerCertificate=False"
  },
  "Cors": {
    "AllowedOrigins": [
      "https://your-production-domain.com"
    ]
  },
  "Jwt": {
    "Issuer": "CricketClub.IdentityApi",
    "Audience": "CricketClub.ApiClients",
    "AccessTokenExpirationMinutes": 60
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "your-production-domain.com;www.your-production-domain.com"
}
